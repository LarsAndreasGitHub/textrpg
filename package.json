{
  "name": "textrpg",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts-ts": "2.15.1",
    "react-shapes": "^0.1.0"
  },
  "scripts": {
    "start:ts": "react-scripts-ts start",
    "start:less": "less-watch-compiler ./src ./public index.less",
    "start": "npm-run-all -p start:*",
    "build:ts": "react-scripts-ts build",
    "build:less": "node ./node_modules/less/bin/lessc/ ./src/index.less ./public/index.css",
    "build": "npm-run-all -p build:*",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "@types/node": "^9.6.6",
    "@types/react": "^16.3.12",
    "@types/react-dom": "^16.0.5",
    "typescript": "^2.8.3"
  }
}
